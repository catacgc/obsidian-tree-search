{
  "compilerOptions": {
    "types": ["react", "node", "obsidian-typings"],
    "baseUrl": ".",
    "jsx": "react-jsx",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
	"strictNullChecks": true,
    "lib": [
      "DOM",
      "ES5",
      "ES6",
      "ES7"
    ]
  },
  "include": [
    "**/*.ts"
  , "src/**.tsx", 
    "src/**.ts"  ]
}
