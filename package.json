{
	"name": "obsidian-tree-search",
	"version": "0.0.9",
	"description": "Quickly search trees of lines in Obsidian",
	"main": "main.js",
	"scripts": {
		"watch:css": "npx tailwindcss -i ./src/styles.css -o ./styles.css --watch",
		"watch:esbuild": "node esbuild.config.mjs",
		"dev": "npm-run-all --parallel watch:*",
		"build:css": "npx tailwindcss -i ./src/styles.css -o ./styles.css --minify",
		"build:esbuild": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build": "npm run build:css && npm run build:esbuild",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.13",
		"@types/markdown-it": "^14.1.2",
		"@types/node": "^16.11.6",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"npm-run-all": "^4.1.5",
		"obsidian": "latest",
		"obsidian-dataview": "^0.4.21",
		"obsidian-typings": "^2.2.0",
		"tailwindcss": "^3.4.12",
		"ts-jest": "^29.1.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"graphology": "^0.25.4",
		"luxon": "^3.5.0",
		"markdown-it": "^14.1.0",
		"promise-worker": "^2.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-markdown": "^9.0.1"
	}
}
